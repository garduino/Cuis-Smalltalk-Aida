	" #noprocess #idle #interrupted "
	state := aSymbol! !

!WebContext methodsFor: 'private-process'!
stopProcess
	self process isNil ifTrue: [^nil].
	self process terminate.
	self process: nil; semaphore: nil.
	self setNoProcessState! !

